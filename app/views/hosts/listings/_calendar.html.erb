<div class="form-group">
<div class="input-daterange">
  <h3>Select dates</h3>
  <input type="text" id="from-date" class="from_date form-control" name="listing[start_date]" placeholder="start date" contenteditable="false">
  <input type="text" id="to-date" class="to_date form-control" name="listing[end_date]" placeholder="end date" contenteditable="false">


  <% if @listing.retired %>
    <p id="retired-listing"><%= "#{@listing.name} has been retired from the listings" %></p>
  <% end %>
  </div>
</div>
