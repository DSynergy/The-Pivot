<%= form_for [:admin, @order] do |f| %>
  <div id="paid" class="radio">
    <%= f.radio_button(:status, "paid") %>
    <%= f.label(:status, "Paid") %>
  </div>
  <div id="cancelled" class="radio">
    <%= f.radio_button(:status, "cancelled") %>
    <%= f.label(:status, "Cancel") %>
  </div>

  <div><%= f.submit "Submit" %></div>
  <% end %>
